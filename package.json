{
  "name": "vscode-xhonker",
  "displayName": "vscode-xhonker",
  "description": "tools",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.35.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "icon": "assets/logo.png",
  "contributes": {
    "commands": [
      {
        "command": "xhonker.terminal",
        "title": "terminal",
        "category": "xhonker"
      },
      {
        "command": "xhonker.process.kill",
        "title": "kill",
        "category": "xhonker"
      },
      {
        "command": "xhonker.process.killNode",
        "title": "killNode",
        "category": "xhonker"
      }
    ],
    "configuration": [
      {
        "title": "xhonker",
        "properties": {
          "xhonker.killType": {
            "default": "node",
            "type": "string"
          },
          "xhonker.killCommand": {
            "default": "kill -9",
            "type": "string"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  }
}
